###################################################
# Batch running of events  
###################################################

# verbosity
/tracking/verbose 0
/control/verbose 0
/run/verbose 0


# GPS Particle Generation
/gps/particle e-
/gps/pos/type Point
/gps/pos/centre 0 0 0
/gps/ang/type iso
/gps/energy 1 MeV

/run/beamOn 1