###################################################
# Batch running of events  
###################################################

# verbosity
/tracking/verbose 0
/control/verbose 0
/run/verbose 0

/RunAction/output/file out.root

# GPS Particle Generation
/gps/particle mu-
/gps/energy 1 MeV
/gps/direction 0 0 -1
/gps/position 0 0 130

/run/beamOn 1