###################################################
# Batch running of events  
###################################################

# verbosity
/tracking/verbose 0
/control/verbose 0
/run/verbose 0

/RunAction/output/file out.root
# GPS Particle Generation
/gps/particle e-
/gps/energy 3 GeV
/gps/direction 0 0 -1
/gps/position 200 300 1000

/run/beamOn 1